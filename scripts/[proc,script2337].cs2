[proc,script2337](component $component0)
if (getwindowmode >= ^windowmode_resizable) {
	if_setposition(calc(if_getwidth(toplevel_fullscreen:com_9) + 10), 10, ^setpos_abs_right, ^setpos_abs_top, $component0);
} else {
	if_setposition(10, 10, ^setpos_abs_right, ^setpos_abs_top, $component0);
}
